package Exercise;

import java.util.Scanner;

public class E5_GCD_subtraction {

	public static int remender_divi(int a, int b) {
		if (a < b)
			return a;
		else 
			return remender_divi(b, a - b);
	}

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.print("Enter two numbers a and b: ");
		int a = scanner.nextInt();
		int b = scanner.nextInt();
		System.out.println("remender of Diivi a and b is: " + remender_divi(a, b));
		scanner.close();
	}
}
