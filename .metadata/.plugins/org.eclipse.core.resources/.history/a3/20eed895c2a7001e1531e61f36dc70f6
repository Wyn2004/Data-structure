package Module;

public class Node<E> implements Comparable<E> {

	public E value;
	public Node<E> left, right;

	public Node(E value) {
		this.value = value;
		this.left = null;
		this.right = null;
	}
	
	public Node(E value, Node<E> left, Node<E> right) {
		this.value = value;
		this.left = left;
		this.right = right;
	}
	
}
